{
  "shape": "䷆",
  "code": "000010",
  "pin_yin": "shī",
  "name": "师",
  "gua_ci": ["贞，丈人吉，无咎。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "师出以律，否臧，凶。"
  },{
    "kind": "九二",
    "desc": "在师中吉，无咎；王三锡命。"
  },{
    "kind": "六三",
    "desc": "师或舆尸，凶。"
  },{
    "kind": "六四",
    "desc": "师左次，无咎。"
  },{
    "kind": "六五",
    "desc": "田有禽，利执言，无咎。长子帅师，弟子舆尸，贞凶。"
  },{
    "kind": "上六",
    "desc": "大君有命，开国承家，小人勿用。"
  }],
  "da_xiang": "地中有水，师；君子以容民畜众。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "师出以律；失律，凶也。"
  },{
    "kind": "九二",
    "desc": "在师中吉，承天宠也。王三锡命，怀万邦也。"
  },{
    "kind": "六三",
    "desc": "师或舆尸，大无功也。"
  },{
    "kind": "六四",
    "desc": "左次无咎，未失常也。"
  },{
    "kind": "六五",
    "desc": "长子帅师，以中行也。弟子舆尸，使不当也。"
  },{
    "kind": "上六",
    "desc": "大君有命，以正功也。小人勿用，必乱邦也。"
  }],
  "tuan_ci": [
    "师，众也。贞，正也。能以众正，可以王矣。刚中而应，行险而顺，以此毒天下，而民从之，吉又何咎矣。"
  ]
}
