{
  "shape": "䷏",
  "code": "001000",
  "pin_yin": "yù",
  "name": "豫",
  "gua_ci": ["利建侯行师。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "鸣豫，凶。"
  },{
    "kind": "六二",
    "desc": "介于石，不终日，贞吉。"
  },{
    "kind": "六三",
    "desc": "盱豫，悔。迟有悔。"
  },{
    "kind": "九四",
    "desc": "由豫，大有得。勿疑。朋盍簪。"
  },{
    "kind": "六五",
    "desc": "贞疾，恒不死。"
  },{
    "kind": "上六",
    "desc": "冥豫，成有渝，无咎。"
  }],
  "da_xiang": "雷出地奋，豫。先王以作乐崇德，殷荐之上帝，以配祖考。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "鸣豫，志穷凶也。"
  },{
    "kind": "六二",
    "desc": "不终日，贞吉；以中正也。"
  },{
    "kind": "六三",
    "desc": "盱豫有悔，位不当也。"
  },{
    "kind": "九四",
    "desc": "由豫，大有得；志大行也。"
  },{
    "kind": "六五",
    "desc": "贞疾，乘刚也。恒不死，中未亡也。"
  },{
    "kind": "上六",
    "desc": "冥豫在上，何可长也。"
  }],
  "tuan_ci": [
    "豫，刚应而志行，顺以动，豫。豫，顺以动，故天地如之，而况建侯行师乎？天地以顺动，故日月不过，而四时不忒；圣人以顺动，则刑罚清而民服。豫之时义大矣哉！"
  ]
}
