{
  "shape": "䷵",
  "code": "001011",
  "pin_yin": "guī mèi",
  "name": "归妹",
  "gua_ci": ["征凶，无攸利。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "归妹以娣，跛能履，征吉。"
  },{
    "kind": "九二",
    "desc": "眇能视，利幽人之贞。"
  },{
    "kind": "六三",
    "desc": "归妹以须，反归以娣。"
  },{
    "kind": "九四",
    "desc": "归妹愆期，迟归有时。"
  },{
    "kind": "六五",
    "desc": "帝乙归妹，其君之袂，不如其娣之袂良，月几望，吉。"
  },{
    "kind": "上六",
    "desc": "女承筐无实，士刲羊无血，无攸利。"
  }],
  "da_xiang": "泽上有雷，归妹；君子以永终知敝。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "归妹以娣，以恒也。跛能履吉，相承也。"
  },{
    "kind": "九二",
    "desc": "利幽人之贞，未变常也。"
  },{
    "kind": "六三",
    "desc": "归妹以须，未当也。"
  },{
    "kind": "九四",
    "desc": "愆期之志，有待而行也。"
  },{
    "kind": "六五",
    "desc": "帝乙归妹，不如其娣之袂良也。其位在中，以贵行也。"
  },{
    "kind": "上六",
    "desc": "上六无实，承虚筐也。"
  }],
  "tuan_ci": [
    "归妹，天地之大义也。天地不交，而万物不兴，归妹人之终始也。说以动，所归妹也。征凶，位不当也。无攸利，柔乘刚也。"
  ]
}
