  "pin_yin": "dà zhuàng",
  "name": "大壮",
  "gua_ci": ["利贞。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "壮于趾，征凶，有孚。"
  },{
    "kind": "九二",
    "desc": "贞吉。"
  },{
    "kind": "九三",
    "desc": "小人用壮，君子用罔，贞厉。羝羊触藩，羸其角。"
  },{
    "kind": "九四",
    "desc": "贞吉悔亡，藩决不羸，壮于大舆之輹。"
  },{
    "kind": "六五",
    "desc": "丧羊于易，无悔。"
  },{
    "kind": "上六",
    "desc": "羝羊触藩，不能退，不能遂，无攸利，艰则吉。"
  }],
  "da_xiang": "雷在天上，大壮；君子以非礼勿履。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "壮于趾，其孚穷也。"
  },{
    "kind": "九二",
    "desc": "贞吉，以中也。"
  },{
    "kind": "九三",
    "desc": "小人用壮，君子罔也。"
  },{
    "kind": "九四",
    "desc": "藩决不羸，尚往也。"
  },{
    "kind": "六五",
    "desc": "丧羊于易，位不当也。"
  },{
    "kind": "上六",
    "desc": "不能退，不能遂，不祥也。艰则吉，咎不长也。"
  }],
  "tuan_ci": [
    "大壮，大者壮也。刚以动，故壮。大壮利贞；大者正也。正大而天地之情可见矣！"
  ]
}
