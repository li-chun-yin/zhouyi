{
  "shape": "䷂",
  "code": "010001",
  "pin_yin": "zhūn",
  "name": "屯",
  "gua_ci": ["元亨，利贞。勿用有攸往，利建侯。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "磐桓，利居贞，利建侯。"
  },{
    "kind": "六二",
    "desc": "屯如邅如，乘马班如，匪寇婚媾，女子贞不字，十年乃字。"
  },{
    "kind": "六三",
    "desc": "即鹿无虞，惟入于林中，君子几不如舍，往吝。"
  },{
    "kind": "六四",
    "desc": "乘马班如，求婚媾，往，吉无不利。"
  },{
    "kind": "九五",
    "desc": "屯其膏；小贞吉，大贞凶。"
  },{
    "kind": "上六",
    "desc": "乘马班如，泣血涟如。"
  }],
  "da_xiang": "云雷，屯；君子以经纶。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "虽磐桓，志行正也。以贵下贱，大得民也。"
  },{
    "kind": "六二",
    "desc": "六二之难，乘刚也。十年乃字，反常也。"
  },{
    "kind": "六三",
    "desc": "即鹿无虞，以从禽也。君子舍之，往吝穷也。"
  },{
    "kind": "六四",
    "desc": "求而往，明也。"
  },{
    "kind": "九五",
    "desc": "屯其膏，施未光也。"
  },{
    "kind": "上六",
    "desc": "泣血涟如，何可长也。"
  }],
  "tuan_ci": [
    "屯，刚柔始交而难生，动乎险中，大亨贞。雷雨之动满盈，天造草昧，宜建侯而不宁。"
  ]
}
