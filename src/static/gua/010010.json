{
  "shape": "䷜",
  "code": "010010",
  "pin_yin": "kǎn",
  "name": "坎",
  "gua_ci": ["习坎，有孚维心，亨。行有尚。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "习坎，入于坎窞，凶。"
  },{
    "kind": "九二",
    "desc": "坎有险，求小得。"
  },{
    "kind": "六三",
    "desc": "来之坎坎，险且枕，入于坎窞，勿用。"
  },{
    "kind": "六四",
    "desc": "樽酒簋贰，用缶，纳约自牖，终无咎。"
  },{
    "kind": "九五",
    "desc": "坎不盈，祗既平，无咎。"
  },{
    "kind": "上六",
    "desc": "系用徽𬙊，寘于丛棘，三岁不得，凶。"
  }],
  "da_xiang": "水洊至，习坎；君子以常德行，习教事。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "习坎入坎，失道凶也。"
  },{
    "kind": "九二",
    "desc": "求小得，未出中也。"
  },{
    "kind": "六三",
    "desc": "来之坎坎，终无功也。"
  },{
    "kind": "六四",
    "desc": "樽酒簋贰，刚柔际也。"
  },{
    "kind": "九五",
    "desc": "坎不盈，中未大也。"
  },{
    "kind": "上六",
    "desc": "上六失道，凶三岁也。"
  }],
  "tuan_ci": [
    "习坎，重险也。水流而不盈，行险而不失其信。维心亨，乃以刚中也。行有尚，往有功也。天险不可升也，地险山川丘陵也，王公设险以守其国，坎之时用大矣哉！"
  ]
}
