  "pin_yin": "jié",
  "name": "节",
  "gua_ci": ["亨。苦节不可贞。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "不出户庭，无咎。"
  },{
    "kind": "九二",
    "desc": "不出门庭，凶。"
  },{
    "kind": "六三",
    "desc": "不节若，则嗟若，无咎。"
  },{
    "kind": "九四",
    "desc": "安节，亨。"
  },{
    "kind": "九五",
    "desc": "甘节，吉；往有尚。"
  },{
    "kind": "上九",
    "desc": "苦节，贞凶，悔亡。"
  }],
  "da_xiang": "泽上有水，节；君子以制数度，议德行。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "不出户庭，知通塞也。"
  },{
    "kind": "九二",
    "desc": "不出门庭，失时极也。"
  },{
    "kind": "六三",
    "desc": "不节之嗟，又谁咎也。"
  },{
    "kind": "九四",
    "desc": "安节之亨，承上道也。"
  },{
    "kind": "九五",
    "desc": "甘节之吉，居位中也。"
  },{
    "kind": "上九",
    "desc": "苦节贞凶，其道穷也。"
  }],
  "tuan_ci": [
    "节，亨，刚柔分，而刚得中。苦节不可贞，其道穷也。说以行险，当位以节，中正以通。天地节而四时成，节以制度，不伤财，不害民。"
  ]
}
