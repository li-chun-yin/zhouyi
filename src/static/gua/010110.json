  "pin_yin": "jǐng",
  "name": "井",
  "gua_ci": ["改邑不改井，无丧无得，往来井。井汔，至亦未繘。井羸其瓶，凶。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "井泥不食，旧井无禽。"
  },{
    "kind": "九二",
    "desc": "井谷射鲋，瓮敝漏。"
  },{
    "kind": "九三",
    "desc": "井渫不食，为我民恻，可用汲，王明，并受其福。"
  },{
    "kind": "六四",
    "desc": "井甃，无咎。"
  },{
    "kind": "九五",
    "desc": "井冽，寒泉食。"
  },{
    "kind": "上六",
    "desc": "井收勿幕，有孚元吉。"
  }],
  "da_xiang": "木上有水，井；君子以劳民劝相。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "井泥不食，下也。旧井无禽，时舍也。"
  },{
    "kind": "九二",
    "desc": "井谷射鲋，无与也。"
  },{
    "kind": "九三",
    "desc": "井渫不食，行恻也。求王明，受福也。"
  },{
    "kind": "六四",
    "desc": "井甃无咎，修井也。"
  },{
    "kind": "九五",
    "desc": "寒泉之食，中正也。"
  },{
    "kind": "上六",
    "desc": "元吉在上，大成也。"
  }],
  "tuan_ci": [
    "巽乎水而上水，井；井养而不穷也。改邑不改井，乃以刚中也。汔至亦未繘井，未有功也。羸其瓶，是以凶也。"
  ]
}
