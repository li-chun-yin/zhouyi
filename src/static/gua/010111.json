  "name": "需",
  "gua_ci": ["有孚，光亨。贞吉，利涉大川。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "需于郊，利用恒，无咎。"
  },{
    "kind": "九二",
    "desc": "需于沙，小有言，终吉。"
  },{
    "kind": "九三",
    "desc": "需于泥，致寇至。"
  },{
    "kind": "六四",
    "desc": "需于血，出自穴。"
  },{
    "kind": "九五",
    "desc": "需于酒食，贞吉。"
  },{
    "kind": "上六",
    "desc": "入于穴，有不速之客三人来，敬之终吉。"
  }],
  "da_xiang": "云上于天，需；君子以饮食宴乐。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "需于郊，不犯难行也。利用恒，无咎，未失常也。"
  },{
    "kind": "九二",
    "desc": "需于沙，衍在中也。虽小有言，以终吉也。"
  },{
    "kind": "九三",
    "desc": "需于泥，灾在外也。自我致寇，敬慎不败也。"
  },{
    "kind": "六四",
    "desc": "需于血，顺以听也。"
  },{
    "kind": "九五",
    "desc": "酒食，贞吉，以中正也。"
  },{
    "kind": "上六",
    "desc": "不速之客来，敬之终吉。虽不当位，未大失也。"
  }],
  "tuan_ci": [
    "需，须也，险在前也；刚健而不陷，其义不困穷矣。需，有孚，光亨，贞吉，位乎天位，以正中也。利涉大川，往有功也。"
  ]
}
