{
  "shape": "䷬",
  "code": "011000",
  "pin_yin": "cuì",
  "name": "萃",
  "gua_ci": ["亨。王假有庙，利见大人；亨，利贞。用大牲吉，利有攸往。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "有孚不终，乃乱乃萃，若号一握为笑，勿恤，往无咎。"
  },{
    "kind": "六二",
    "desc": "引吉，无咎，孚乃利用禴。"
  },{
    "kind": "六三",
    "desc": "萃如，嗟如，无攸利，往无咎，小吝。"
  },{
    "kind": "九四",
    "desc": "大吉，无咎。"
  },{
    "kind": "九五",
    "desc": "萃有位，无咎。匪孚，元永贞，悔亡。"
  },{
    "kind": "上六",
    "desc": "赍咨涕洟，无咎。"
  }],
  "da_xiang": "泽上于地，萃；君子以除戎器，戒不虞。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "乃乱乃萃，其志乱也。"
  },{
    "kind": "六二",
    "desc": "引吉无咎，中未变也。"
  },{
    "kind": "六三",
    "desc": "往无咎，上巽也。"
  },{
    "kind": "九四",
    "desc": "大吉无咎，位不当也。"
  },{
    "kind": "九五",
    "desc": "萃有位，志未光也。"
  },{
    "kind": "上六",
    "desc": "赍咨涕洟，未安上也。"
  }],
  "tuan_ci": [
    "萃，聚也；顺以说，刚中而应，故聚也。王假有庙，致孝享也。利见大人亨，聚以正也。用大牲吉，利有攸往，顺天命也。观其所聚，而天地万物之情可见矣。"
  ]
}
