  },{
    "kind": "九二",
    "desc": "孚兑之吉，信志也。"
  },{
    "kind": "六三",
    "desc": "来兑之凶，位不当也。"
  },{
    "kind": "九四",
    "desc": "九四之喜，有庆也。"
  },{
