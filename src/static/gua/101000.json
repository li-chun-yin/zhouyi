  "pin_yin": "jìn",
  "name": "晋",
  "gua_ci": ["康侯用锡马蕃庶，昼日三接。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "晋如，摧如，贞吉。罔孚，裕无咎。"
  },{
    "kind": "六二",
    "desc": "晋如，愁如，贞吉。受兹介福，于其王母。"
  },{
    "kind": "六三",
    "desc": "众允，悔亡。"
  },{
    "kind": "九四",
    "desc": "晋如鼫鼠，贞厉。"
  },{
    "kind": "六五",
    "desc": "悔亡，失得勿恤，往吉无不利。"
  },{
    "kind": "上九",
    "desc": "晋其角，维用伐邑，厉吉无咎，贞吝。"
  }],
  "da_xiang": "明出地上，晋；君子以自昭明德。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "晋如，摧如；独行正也。裕无咎；未受命也。"
  },{
    "kind": "六二",
    "desc": "受之介福，以中正也。"
  },{
    "kind": "六三",
    "desc": "众允之，志上行也。"
  },{
    "kind": "九四",
    "desc": "鼫鼠贞厉，位不当也。"
  },{
    "kind": "六五",
    "desc": "失得勿恤，往有庆也。"
  },{
    "kind": "上九",
    "desc": "维用伐邑，道未光也。"
  }],
  "tuan_ci": [
    "晋，进也。明出地上，顺而丽乎大明，柔进而上行。是以康侯用锡马蕃庶，昼日三接也。"
  ]
}
