{
  "shape": "䷴",
  "code": "110100",
  "pin_yin": "jiàn",
  "name": "渐",
  "gua_ci": ["女归吉，利贞。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "鸿渐于干，小子厉，有言，无咎。"
  },{
    "kind": "六二",
    "desc": "鸿渐于磐，饮食衎衎，吉。"
  },{
    "kind": "九三",
    "desc": "鸿渐于陆，夫征不复，妇孕不育，凶；利御寇。"
  },{
    "kind": "六四",
    "desc": "鸿渐于木，或得其桷，无咎。"
  },{
    "kind": "九五",
    "desc": "鸿渐于陵，妇三岁不孕，终莫之胜，吉。"
  },{
    "kind": "上九",
    "desc": "鸿渐于逵，其羽可用为仪，吉。"
  }],
  "da_xiang": "山上有木，渐；君子以居贤德，善俗。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "小子之厉，义无咎也。"
  },{
    "kind": "六二",
    "desc": "饮食衎衎，不素饱也。"
  },{
    "kind": "九三",
    "desc": "夫征不复，离群丑也。妇孕不育，失其道也。利用御寇，顺相保也。"
  },{
    "kind": "六四",
    "desc": "或得其桷，顺以巽也。"
  },{
    "kind": "九五",
    "desc": "终莫之胜，吉；得所愿也。"
  },{
    "kind": "上九",
    "desc": "其羽可用为仪，吉；不可乱也。"
  }],
  "tuan_ci": [
    "渐之进也，女归吉也。进得位，往有功也。进以正，可以正邦也。其位刚，得中也。止而巽，动不穷也。"
  ]
}
