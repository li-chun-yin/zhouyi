{
  "shape": "䷈",
  "code": "110111",
  "pin_yin": "xiǎo chù",
  "name": "小畜",
  "gua_ci": ["亨。密云不雨，自我西郊。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "复自道，何其咎，吉。"
  },{
    "kind": "九二",
    "desc": "牵复，吉。"
  },{
    "kind": "九三",
    "desc": "舆说辐，夫妻反目。"
  },{
    "kind": "六四",
    "desc": "有孚，血去惕出，无咎。"
  },{
    "kind": "九五",
    "desc": "有孚挛如，富以其邻。"
  },{
    "kind": "上九",
    "desc": "既雨既处，尚德载，妇贞厉，月几望，君子征凶。"
  }],
  "da_xiang": "风行天上，小畜，君子以懿文德。",
  "xiao_xiang": [{
    "kind": "初九",
    "desc": "复自道，其义吉也。"
  },{
    "kind": "九二",
    "desc": "牵复在中，亦不自失也。"
  },{
    "kind": "九三",
    "desc": "夫妻反目，不能正室也。"
  },{
    "kind": "六四",
    "desc": "有孚。惕出。上合志也。"
  },{
    "kind": "九五",
    "desc": "有孚挛如，不独富也。"
  },{
    "kind": "上九",
    "desc": "既雨既处，德积载也。君子征凶，有所疑也。"
  }],
  "tuan_ci": [
    "小畜，柔得位而上下应之，曰小畜。健而巽，刚中而志行，乃亨。密云不雨，尚往也。自我西郊，施未行也。"
  ]
}
