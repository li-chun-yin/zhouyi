  "name": "履",
  "gua_ci": ["履虎尾，不咥人，亨。"],
  "yao_ci": [{
    "kind": "初九",
    "desc": "素履，往无咎。"
  },{
    "kind": "九二",
    "desc": "履道坦坦，幽人貞吉。"
  },{
    "kind": "六三",
    "desc": "眇能视，跛能履，履虎尾，咥人，凶。武人为于大君。"
  },{
    "kind": "九四",
    "desc": "履虎尾，诉诉终吉。"
  },{
    "kind": "九五",
    "desc": "夬履，贞厉。"
  },{
