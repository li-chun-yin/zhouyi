  "yao_ci": [{
    "kind": "初九",
    "desc": "同人于门，无咎。"
  },{
    "kind": "六二",
    "desc": "同人于宗，吝。"
  },{
    "kind": "九三",
    "desc": "伏戎于莽，升其高陵，三岁不兴。"
  },{
    "kind": "九四",
    "desc": "乘其墉，弗克，攻吉。"
  },{
