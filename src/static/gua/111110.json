  "pin_yin": "gòu",
  "name": "姤",
  "gua_ci": ["女壮，勿用取女。"],
  "yao_ci": [{
    "kind": "初六",
    "desc": "系于金柅，贞吉，有攸往，见凶，羸豕踟躅。"
  },{
    "kind": "九二",
    "desc": "包有鱼，无咎，不利宾。"
  },{
    "kind": "九三",
    "desc": "臀无肤，其行次且，厉，无大咎。"
  },{
    "kind": "九四",
    "desc": "包无鱼，起凶。"
  },{
    "kind": "九五",
    "desc": "以杞包瓜，含章，有陨自天。"
  },{
    "kind": "上九",
    "desc": "姤其角，吝，无咎。"
  }],
  "da_xiang": "天下有风，姤；后以施命诰四方。",
  "xiao_xiang": [{
    "kind": "初六",
    "desc": "系于金柅，柔道牵也。"
  },{
    "kind": "九二",
    "desc": "包有鱼，义不及宾也。"
  },{
    "kind": "九三",
    "desc": "其行次且，行未牵也。"
  },{
    "kind": "九四",
    "desc": "无鱼之凶，远民也。"
  },{
    "kind": "九五",
    "desc": "含章，中正也。有陨自天，志不舍命也。"
  },{
    "kind": "上九",
    "desc": "姤其角，上穷吝也。"
  }],
  "tuan_ci": [
    "姤，遇也，柔遇刚也。勿用取女，不可与长也。天地相遇，品物咸章也。刚遇中正，天下大行也。姤之时义大矣哉！"
  ]
}
